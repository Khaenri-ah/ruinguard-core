{
  "name": "@ruinguard/core",
  "version": "2.0.0",
  "description": "The core of the RuinGuard modular bot system",
  "keywords": [
    "modular",
    "discord",
    "bot",
    "discord.js",
    "ruinguard"
  ],
  "homepage": "https://github.com/Khaenri-ah/ruinguard-core#readme",
  "bugs": "https://github.com/Khaenri-ah/ruinguard-core/issues",
  "type": "module",
  "author": {
    "name": "AroLeaf",
    "url": "https://leaf.moe/"
  },
  "contributors": [
    {
      "name": "MRDGH2821",
      "url": "https://bit.ly/mrdgh2821"
    }
  ],
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Khaenri-ah/ruinguard-core.git"
  },
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "scripts": {
    "fix": "eslint --fix",
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc --outDir dist/cjs --module commonjs --removeComments",
    "build:esm": "tsc --outDir dist/esm --module es2020 --removeComments",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir ./types",
    "build:docs": "npx typedoc && mkdocs build -f docs/mkdocs.yml"
  },
  "dependencies": {
    "discord.js": "^13.4.0",
    "keyv": "^4.0.4",
    "node-fetch": "^2.6.7",
    "xregexp": "^5.1.0"
  },
  "devDependencies": {
    "@types/keyv": "^3.1.3",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.3.0",
    "eslint-config-ruinguard": "^1.0.0",
    "typedoc": "^0.22.11",
    "typedoc-plugin-markdown": "^3.11.14",
    "typedoc-plugin-mdn-links": "^1.0.5",
    "typescript": "^4.6.2"
  },
  "files": [
    "dist/**/*",
    "types/**/*",
    "index.*js"
  ],
  "types": "types/index.d.ts"
}
